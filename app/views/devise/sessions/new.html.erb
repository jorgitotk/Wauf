<%= devise_error_messages! %>
<div class="col-md-4"></div>
<div class="col-md-4 margin-signin">
  <h2>Iniciar Sesión</h2>
  <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
    <div class="field">
      <%= f.label :login, 'CÓDIGO' %><br />
      <%= f.text_field :login, class:"form-control", autofocus: true %>
    </div>
    <div class="field">
      <%= f.label :password, 'CONTRASEÑA' %><br />
      <%= f.password_field :password, class:"form-control", autocomplete: "off" %>
    </div>
    <div class="actions margin-login">
      <%= f.submit "Iniciar Sesión", class:"btn btn-primary" %>
    </div>
  <% end %>
</div>
<div class="col-md-4"></div>