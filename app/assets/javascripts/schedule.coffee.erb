@select_time = (obj, row, col) ->
	$(obj).toggleClass("green");
	is_checked_days = $("#dayc-" + row + "-r-" + col).prop("checked");
	is_checked_hours = $("#hourc-" + row + "-r-" + col).prop("checked");
	if is_checked_days
		$("#dayc-" + row + "-r-" + col).prop("checked", false)
	else
		$("#dayc-" + row + "-r-" + col).prop("checked", true)
	if is_checked_hours
		$("#hourc-" + row + "-r-" + col).prop("checked", false)
	else
		$("#hourc-" + row + "-r-" + col).prop("checked", true)

@set_courses = ->
  id = $('#faculty-type-select option:selected').val()
  if id == '' then id = 0
  $.ajax 'schedules/' + id + '/set_courses',
    type: 'POST'
    dataType: 'script'